{
  "cSpell.language": "en,ru",
  "cSpell.enabled": false,
  "cSpell.diagnosticLevel": "Hint",
  "cSpellExt.enableDictionaries": [
    "russian"
  ],
  "Lua.diagnostics.globals": [
    "vim",
    "use"
  ],
  "clangd.enabled": true,
  "coc.preferences.enableFloatHighlight": true,
  "codeLens.subseparator": " | ",
  "diagnostic.displayByAle": true,
  "diagnostic.enable": true,
  "diagnostic.messageDelay": 100,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "python.linting.enabled": false,
  "rust-analyzer.debug.runtime": "vimspector",
  "rust-analyzer.debug.vimspector.configuration.name": "launch",
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.lens.implementations": true,
  "rust-analyzer.lens.methodReferences": true,
  "rust-analyzer.lens.references": false,
  "rust-analyzer.lens.run": true,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.server.path": "/usr/bin/rust-analyzer",
  "snippets.ultisnips.enable": true,
  "suggest.numberSelect": true,
  "sumneko-lua.enableNvimLuaDev": true,
  "suggest.snippetIndicator": "~",
  "yank.highlight.duration": 300,
  "cSpell.userWords": [
    "clangd",
    "vimt"
  ]
}
